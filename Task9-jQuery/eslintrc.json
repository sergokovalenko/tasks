{
  "env": {
    "browser": true,
    "jQuery": true,
    "es6": true,
    "amd": true
  },
  "globals": {
    "_": true,
    "window": true,
    "$": true
  },
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": false
    }
  },
  "rules": {
    "no-multiple-empty-lines": [
      2,
      { "max": 2 }
    ],
    "newline-after-var": 2,
    "no-extra-strict": 0,
    "no-else-return": 2,
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "no-throw-literal": 2,
    "no-extra-parens": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-unexpected-multiline": 2,
    "default-case": 2,
    "dot-location": [
      2,
      "property"
    ],
    "radix": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "brace-style": [2, "1tbs", {
            "allowSingleLine": false
        }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "eqeqeq": [
      2,
      "smart"
    ],
    "curly": 2,
    "one-var-declaration-per-line": [ 2, "always" ],
    "no-empty": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-duplicate-case": 2,
    "no-fallthrough": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-unreachable": 2,
    "indent": [
      2,
      4,
      {
        "VariableDeclarator": {
          "var": 1,
          "let": 2,
          "const": 3
        },
        "SwitchCase": 1
      }
    ],
    "no-redeclare": [
      2,
      {
        "builtinGlobals": true
      }
    ],
    "keyword-spacing": [
      2
    ],
    "no-self-compare": 2,
    "no-shadow-restricted-names": 2,
    "new-cap": 2,
    "no-trailing-spaces": 2,
    "space-before-blocks": 2,
    "semi-spacing": 2,
    "semi": [
      2,
      "always"
    ]
  }
}
